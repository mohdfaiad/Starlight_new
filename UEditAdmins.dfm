  object cdsQuery: TOraQuery
    SQL.Strings = (
      
        'select decode(b.USER_ID,null,0,1) as d_checked, c.nick, c.fio, d' +
        '.id_debetors, d.id_clients, g.NAME as group_name, c.id_clients_g' +
        'roups, d.ID_OFFICE'
      '    FROM DEBETORS D'
      '    inner join CLIENTS C on C.ID_CLIENTS = D.ID_CLIENTS'
      
        '    inner join CLIENTS_GROUPS G on G.ID_CLIENTS_GROUPS = C.ID_CL' +
        'IENTS_GROUPS'
      
        '    left outer join DEBETOR_PERMISSIONS b on b.DEBETOR_ID = d.id' +
        '_debetors and b.USER_ID = :user_id'
      'WHERE d.id_office = :office_id '
      'order by group_name, fio')
    Left = 16
    Top = 480
    ParamData = <
